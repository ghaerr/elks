	libtool) CXX_LIB_SUFFIX='.la'  ;;
	normal)  CXX_LIB_SUFFIX='.a'   ;;
	debug)   CXX_LIB_SUFFIX='_g.a' ;;
	profile) CXX_LIB_SUFFIX='_p.a' ;;
	shared)
		case $cf_cv_system_name in
		hpux*)	CXX_LIB_SUFFIX='.sl'  ;;
		*)	CXX_LIB_SUFFIX='.so'  ;;
